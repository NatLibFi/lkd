        "namn av resurs"@sv ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( lkd:Work lkd:Expression lkd:Manifestation lkd:Item lkd:Event ) ] ;
    rdfs:range lkd:Title ;
    skos:exactMatch bf:title ;
    skos:narrowMatch rdax:P00019 .

lkd:Title a owl:Class ;
    rdfs:label "nimeke"@fi,
        "titel"@sv ;
    skos:broadMatch rdac:C10012 ;
    skos:exactMatch bf:Title .

lkd:Item a owl:Class ;
    rdfs:label "kappale"@fi,
        "utgiven med"@sv ;
    skos:exactMatch bf:Item,
        rdac:C10003 .

lkd:Expression a owl:Class ;
    rdfs:label "ekspressio"@fi,
        "ekspression"@sv ;
    rdfs:subClassOf bf:Work ;
    skos:broadMatch bf:Work ;
    skos:exactMatch rdac:C10006 .

lkd:Work a owl:Class ;
    rdfs:label "teos"@fi,
        "verk"@sv ;
    rdfs:subClassOf bf:Work ;
    skos:broadMatch bf:Work ;
    skos:exactMatch rdac:C10001 .

lkd:Manifestation a owl:Class ;
    rdfs:label "manifestaatio"@fi,
        "manifestation"@sv ;
    skos:exactMatch bf:Instance ;
    skos:narrowMatch rdac:C10007 .

